<div class="displayMembersContainer">
    <div class="searchBarContainer">
       <input placeholder="Member Name" class="search-bar" @bind="searchedMemberName" @ref="searchBar" @bind:event="oninput" @onkeyup="searchMember"/>
       <svg class="searchIcon" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" stroke="#000000"><g id="SVGRepo_bgCarrier" stroke-width="0"></g><g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g><g id="SVGRepo_iconCarrier"> <path d="M11 6C13.7614 6 16 8.23858 16 11M16.6588 16.6549L21 21M19 11C19 15.4183 15.4183 19 11 19C6.58172 19 3 15.4183 3 11C3 6.58172 6.58172 3 11 3C15.4183 3 19 6.58172 19 11Z" stroke="#000000" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"></path> </g></svg>
     </div>
   
    <div class="displayTeamsButtonContainer">
        
        <div class="displayTeamsButtonsContainer">
           <svg class="displayTeamsButtons" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" id="add-user"><path d="M6.55 7.12c0 3.64 2.97 6.61 6.62 6.61s6.62-2.97 6.62-6.61c0-3.65-2.97-6.62-6.62-6.62S6.55 3.47 6.55 7.12zM1.67 26v2c0 1.93 1.57 3.5 3.5 3.5h16c1.93 0 3.5-1.57 3.5-3.5v-2c0-6.34-5.16-11.5-11.5-11.5S1.67 19.66 1.67 26zm25.543-10.651v-1.617h1.616a1.5 1.5 0 0 0 0-3h-1.616V9.116a1.5 1.5 0 0 0-3 0v1.616h-1.615a1.5 1.5 0 0 0 0 3h1.615v1.617a1.5 1.5 0 0 0 3 0z"></path></svg>
           <label class="displayTeamsButtonsCreateTeam">Create Member</label>
        </div>

    </div> 
         
         
         <div class="displayMembersCardsContainer">
      @foreach (var member in searchedMembers)
      {
         <MemberCard member="member"></MemberCard>
      }
   </div>
   
</div>